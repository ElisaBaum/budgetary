{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": ["es2016", "esnext", "esnext.asynciterable"],
    "sourceMap": true,

    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,

    "experimentalDecorators": true,
